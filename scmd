newcmd() { echo "alias $1='$2'" >> ${BASH_SOURCE[0]}; }
vedit() { vi ${BASH_SOURCE[0]}; }
nedit() { nano ${BASH_SOURCE[0]}; }
ocmd() { open ${BASH_SOURCE[0]}; }
alias lsbranch='git branch'
alias cbranch='git symbolic-ref --short HEAD'
alias newbranch='git checkout -b'
alias rmbranch='git branch -D'
alias cpbn='cbranch| pbcopy'
alias gst='git status'
alias addall='git add .'
alias gcmit='git commit -m'
alias gacmit='git commit -am'
alias gcmitall='git add . && git commit -m'
alias gmrg='git merge'
mergein() { THIS=$(cbranch); git checkout $1; git merge $THIS;  }
alias rebasewith='git rebase'
rebasein() { THIS=$(cbranch); git checkout $1; git rebase $THIS;  }
alias rbc='git add . && git rebase --continue'
alias rmchng='git checkout .'
alias rmall='git checkout . && git clean -f'
alias rmf='git checkout'
alias gpull='git pull'
alias pullr='git pull --rebase'
opull() { git pull origin predev; }
alias gpush='git push'
opush() { git push origin predev; }
alias gl='git log'
alias gdf='git diff'
alias resetchng='git reset HEAD'
alias resetlastcmit='git reset HEAD~'
# Please do not modify any of command above this line
alias reload='source ~/.bash_profile'

# below this line you can add your custom commands and functions
alias gotomnm='cd ~/development/mnmbackend/ && workon mnmbackend'
alias predev='git checkout predev'
alias billdesk='git checkout bill-desk-integration'
alias billtest='git checkout billdesk-test-deploy'
alias nodal='git checkout nodal-accout-integration'
alias nodaltest='git checkout nodal-with-predev'
alias shell='bash /Users/vishalnagda/development/run_shell.sh'
alias runmnm='bash /Users/vishalnagda/development/deploy-mnm.sh'
alias gotoexp='cd /Users/vishalnagda/development/experiments/ && workon experiment'
alias mnmlg='tail -f /Users/vishalnagda/development/mnmbackend/log/debug.log'
